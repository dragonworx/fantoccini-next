    "better-docs": "^2.7.3",
    "jsdoc": "^4.0.4",
    "svelte": "^4.2.17",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "vite-plugin-devtools-json": "^0.2.1",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@fontsource/fira-mono": "^5.2.6",
    "@fontsource/orbitron": "^5.2.6"
